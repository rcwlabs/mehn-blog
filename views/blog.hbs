{{message}}

<div class="blog-section">
    <div class="blog-post">
        <div 
            style="background: url('{{post.headerImg}}');"
            class="post-header">

        </div>
        <div class="post-content">
            <h3 class="post-title">{{post.title}}</h3>
            <p class="post-author">by: {{post.author}}</p>
            {{post.content}}
        </div>
    </div>
</div>